<div class="form-group row">
    <label class="col-form-label col-sm-2" for="ID">ID</label>
    <div class="col-sm-8">
      <input autofocus class="form-control" name="ID" value="<%= customer.ID %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusFname">cusFname</label>
    <div class="col-sm-7">
      <input class="form-control" name="cusFname" value="<%= customer.cusFname %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusLname">cusLname</label>
    <div class="col-sm-7">
      <input class="form-control" name="cusLname" value="<%= customer.cusLname %>" />
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusState">State</label>
    <div class="col-sm-7">
      <input class="form-control" name="cusState" value="<%= customer.cusState %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="cusSalesYTD">Sales YTD</label>
    <div class="col-sm-10">
        <input class="form-control" name="cusSalesYTD" value="<%= customer.cusSalesYTD %>" />
    </div>

    <div class="form-group row">
        <label class="col-form-label col-sm-2" for="cusSalesPrev">Sales Prior</label>
        <div class="col-sm-10">
            <input class="form-control" name="cusSalesPrev" value="<%= customer.SalesPrev %>" />
        </div>

  </div>